---
import { getEntry } from 'astro:content';
import PageLayout from '@/layouts/CommonPage.astro';

const aboutEntry = await getEntry('pages', 'about');
const rendered = aboutEntry ? await aboutEntry.render() : null;
---

{
  rendered && aboutEntry && (
    <PageLayout title={aboutEntry.data.title} headings={rendered.headings}>
      <rendered.Content />
    </PageLayout>
  )
}
